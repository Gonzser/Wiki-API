<%- include("partials/header") -%>

<h1>COMPOSE</h1>
<form  action="/compose" method="post">
  <div class="form-group">
    <label for="">Title</label>
    <input class="form-control" type="text" name="titlePost" value="">
    <small>Try to be desireble</small>
  </div>
  <div class="form-group">
    <label for="">Post</label>
    <textarea class="form-control" name="lastPost" rows="8" cols="80"></textarea>
  </div>
  <div class="form-group">
    <button class="btn btn-primary" type="submit" name="button" value="publish">Publish</button>
  </div>
</form>

<%- include("partials/footer") -%>
